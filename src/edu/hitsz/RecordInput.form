<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="edu.hitsz.RecordInput">
  <grid id="27dc6" binding="mainPanel" layout-manager="GridBagLayout">
